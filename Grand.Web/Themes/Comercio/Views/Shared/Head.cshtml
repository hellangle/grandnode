@using Grand.Core;
@using Grand.Framework.Themes
@using Grand.Framework.UI
@using Grand.Services.Helpers;
@inject IWorkContext workContext
@inject IThemeContext themeContext
@inject IUserAgentHelper userAgentHelper
@{
    var supportRtl = workContext.WorkingLanguage.Rtl;
    var themeName = themeContext.WorkingThemeName;

    //add browser specific CSS files
    if (supportRtl)
    {
    }

    <!-- <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet"> -->
    Html.AppendCssFileParts(ResourceLocation.Head, string.Format("~/Themes/{0}/Content/dependencies/bootstrap/css/bootstrap.min.css", themeName));
    Html.AppendCssFileParts(ResourceLocation.Head, string.Format("~/Themes/{0}/Content/dependencies/fontawesome/css/fontawesome-all.min.css", themeName));
    Html.AppendCssFileParts(ResourceLocation.Head, string.Format("~/Themes/{0}/Content/dependencies/owl.carousel/css/owl.carousel.min.css", themeName));
    Html.AppendCssFileParts(ResourceLocation.Head, string.Format("~/Themes/{0}/Content/dependencies/flaticon/css/flaticon.css", themeName));
    Html.AppendCssFileParts(ResourceLocation.Head, string.Format("~/Themes/{0}/Content/dependencies/wow/css/animate.css", themeName));

    Html.AppendCssFileParts(ResourceLocation.Head, string.Format("~/Themes/{0}/Content/dependencies/jquery-ui/css/jquery-ui.css", themeName));
    Html.AppendCssFileParts(ResourceLocation.Head, string.Format("~/Themes/{0}/Content/dependencies/venobox/css/venobox.css", themeName));
    Html.AppendCssFileParts(ResourceLocation.Head, string.Format("~/Themes/{0}/Content/dependencies/slick-carousel/css/slick.css", themeName));
	<!-- Site Stylesheet -->
    Html.AppendCssFileParts(ResourceLocation.Head, string.Format("~/Themes/{0}/Content/assets/css/app.css", themeName));

    <!-- Dependency Scripts -->
    Html.AppendScriptParts(ResourceLocation.Footer, string.Format("~/Themes/{0}/Content/dependencies/jquery/jquery.min.js", themeName));
    Html.AppendScriptParts(ResourceLocation.Footer, string.Format("~/Themes/{0}/Content/dependencies/popper.js/popper.min.js", themeName));
    Html.AppendScriptParts(ResourceLocation.Footer, string.Format("~/Themes/{0}/Content/dependencies/bootstrap/js/bootstrap.min.js", themeName));
    Html.AppendScriptParts(ResourceLocation.Footer, string.Format("~/Themes/{0}/Content/dependencies/owl.carousel/js/owl.carousel.min.js", themeName));
    Html.AppendScriptParts(ResourceLocation.Footer, string.Format("~/Themes/{0}/Content/dependencies/wow/js/wow.min.js", themeName));
    Html.AppendScriptParts(ResourceLocation.Footer, string.Format("~/Themes/{0}/Content/dependencies/isotope-layout/js/isotope.pkgd.min.js", themeName));
    Html.AppendScriptParts(ResourceLocation.Footer, string.Format("~/Themes/{0}/Content/dependencies/imagesloaded/js/imagesloaded.pkgd.min.js", themeName));
    Html.AppendScriptParts(ResourceLocation.Footer, string.Format("~/Themes/{0}/Content/dependencies/jquery.countdown/js/jquery.countdown.min.js", themeName));
    Html.AppendScriptParts(ResourceLocation.Footer, string.Format("~/Themes/{0}/Content/dependencies/gmap3/js/gmap3.min.js", themeName));
    Html.AppendScriptParts(ResourceLocation.Footer, string.Format("~/Themes/{0}/Content/dependencies/venobox/js/venobox.min.js", themeName));

    Html.AppendScriptParts(ResourceLocation.Footer, string.Format("~/Themes/{0}/Content/dependencies/slick-carousel/js/slick.js", themeName));
    Html.AppendScriptParts(ResourceLocation.Footer, string.Format("~/Themes/{0}/Content/dependencies/headroom/js/headroom.js", themeName));
    Html.AppendScriptParts(ResourceLocation.Footer, string.Format("~/Themes/{0}/Content/dependencies/jquery-ui/js/jquery-ui.min.js", themeName));
	
	<!--Google map api -->
	<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBsBrMPsyNtpwKXPPpG54XwJXnyobfMAIc"></script>
	<!-- Site Scripts -->
    Html.AppendScriptParts(ResourceLocation.Footer, string.Format("~/Themes/{0}/assets/js/venobox/js/app.js", themeName));
}